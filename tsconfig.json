{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@application/*": ["application/*"],
      "@controllers/*": ["controllers/*"],
      "@DI/*": ["DI/*"],
      "@domain/*": ["domain/*"],
      "@infra/*": ["infra/*"],
      "@main/*": ["main/*"],
      "@utils/*": ["utils/*"]
    },
    "rootDirs": ["src"],
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
}
